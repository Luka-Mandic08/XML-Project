{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@frontend/features/flights/home/container": [
        "libs/features/flights/home/container/src/index.ts"
      ],
      "@frontend/features/flights/home/data-access": [
        "libs/features/flights/home/data-access/src/index.ts"
      ],
      "@frontend/features/shared/container": [
        "libs/features/shared/container/src/index.ts"
      ],
      "@frontend/features/shared/data-access": [
        "libs/features/shared/data-access/src/index.ts"
      ],
      "@frontend/helpers": ["libs/helpers/src/index.ts"],
      "@frontend/models": ["libs/models/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
